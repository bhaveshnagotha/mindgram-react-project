{
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": false,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "varsIgnorePattern": "[I]\\w+"
      }
    ],
    "quotemark": [true, "single", "jsx-double", "avoid-escape"]
  }
}
